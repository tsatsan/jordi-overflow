{
  "name": "platzi-overflow",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -r \"npm run start:server\"  \"npm run start:client\"",
    "start:server": "NODE_ENV=development DEBUG=platzi-overflow* nodemon server/index.js  --exec babel-node",
    "start:client": "ng serve",
    "start-prod": "NODE_ENV=production node server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "npm run build:client && npm run build:server",
    "build:client": "ng build --environment=prod",
    "build:server": "babel server --out-dir dist/server",
    "postinstall": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.6",
    "@angular/cdk": "^5.2.3",
    "@angular/cli": "~1.7.1",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.3",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "angular-moment": "^1.2.0",
    "angular2-moment": "^1.8.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "core-js": "^2.4.1",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^8.2.0",
    "moment": "^2.20.1",
    "mongo-hacker": "0.0.15",
    "mongoose": "^5.0.10",
    "mongoose-unique-validator": "^2.0.0",
    "nodemon": "^1.17.1",
    "rxjs": "^5.5.6",
    "sweet-scroll": "^3.0.0",
    "typescript": "~2.5.3",
    "url-join": "^4.0.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/language-service": "^5.2.0",
